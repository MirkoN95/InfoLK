package sportabteilung;

/**
 *
 * @author Schüler
 */
public class Mitglieder {

    public String Name;
    public String Anschrift;
    public boolean Schuhe;
    public Mitglieder (String NName, String AAnschrift, boolean Schuhe){
            Name=NName;
            Anschrift=AAnschrift;
            
        }
    
    public static void main(String[] args) {
        
            Mitglieder m1 = new Mitglieder("Peter", "Paulinstraße",true);
            Mitglieder m2 = new Mitglieder("Hans", "Saarstraße");
            Mitglieder m3 = new Mitglieder("Jürgen", "Römerstraße");
            
        
    }
    
}
